    <section class="for-full-back color-white " id="contact-inner">
        <div class="container">
            <div class="row">
                <div class="col-md-5 contact-cls">
                    <h2>GET IN TOUCH</h2>
                    <div>


                        <span><i class="fa fa-home"></i>Address: <b>RDX GYM </b> B-223, II Floor, Sec-7, Opp. IGNOU Center, Dwarka, Delhi - 110075</span>
                        <br>
                        <span><i class="fa fa-phone"></i>Phone: +91-(999)-939-9915</span>
                        <br>
                        <span><i class="fa fa-envelope-o"></i>support@myfitnessclub.in</span>
                        <br>
                        <span><i class="fa fa-globe"></i>www.myfitnessclub.in</span>
                        <br>
                    </div>
                </div>
                <div class="col-md-7">
                    <p></p>
                    
                    <%= form_for @welcome, url:   welcomes_path ,:method=>:POST , :remote=>true do |f|%>

                   <form>
                        <div class="row">
                            <div class="col-md-6 col-sm-6">
                                <div class="form-group">
                                   
                                      <%= f.text_field :first_name, placeholder:"First Name" ,id:"first_name", :class=>"form-control text-flid text-lable valid_title_field " , required:"required" %>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6">
                                <div class="form-group">
                                  
                                    <%= f.email_field :email, placeholder:"Email" ,id:"Email", :class=>"form-control text-flid text-lable valid_title_field " , required:"required" %>
                                   
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4 col-sm-4">
                                <div class="form-group">
                                   
                                      <%= f.text_field :phone, placeholder:"Mobile Number" , pattern:"[789][0-9]{9}",id:"phone", :class=>"form-control text-flid text-lable valid_title_field " , required:"required" %>
                                </div>
                            </div>
                            <div class="col-md-8 col-sm-8">
                                <div class="form-group">
                                  
                                    <%= f.text_field :address, placeholder:"Address" ,id:"address", :class=>"form-control text-flid text-lable valid_title_field " , required:"required" %>
                                   
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 col-sm-12">
                                <div class="form-group">
                                   
 
                                    <%= f.text_area :message, placeholder:"Message" ,id:"message", :class=>"form-control text-flid text-lable valid_title_field " , required:"required" %>

                                </div>
                                <div class="form-group">
                                    <button type="submit"  class="btn btn-default " data-disable-with="Submitting...">Submit Request</button>
                                </div>
                            </div>
                        </div>
                    </form> 
                    <% end %>
                </div>
            </div>
        </div>
    </section>

